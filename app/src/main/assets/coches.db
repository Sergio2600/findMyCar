BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "USUARIOS" (
	"ID"	INTEGER NOT NULL,
	"USUARIO"	TEXT NOT NULL,
	"PASSWORD"	TEXT NOT NULL,
	PRIMARY KEY("ID" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "ETIQUETAS" (
	"ID"	INTEGER NOT NULL,
	"ETIQUETA"	TEXT NOT NULL,
	"IMAGEN"	TEXT,
	PRIMARY KEY("ID" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "TRACCION" (
	"ID"	INTEGER NOT NULL,
	"TRACCION"	INTEGER NOT NULL,
	PRIMARY KEY("ID" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "TIPO_CARROCERIA" (
	"ID"	INTEGER NOT NULL,
	"TIPO_CARROCERIA"	INTEGER NOT NULL,
	PRIMARY KEY("ID" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "TIPO_CAMBIO" (
	"ID"	INTEGER NOT NULL,
	"TIPO_CAMBIO"	INTEGER NOT NULL,
	PRIMARY KEY("ID" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "COMBUSTIBLE" (
	"ID"	INTEGER NOT NULL,
	"COMBUSTIBLE"	INTEGER NOT NULL,
	PRIMARY KEY("ID" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "COCHES" (
	"ID"	INTEGER NOT NULL,
	"MARCA"	TEXT NOT NULL,
	"MODELO"	TEXT NOT NULL,
	"VERSION"	TEXT NOT NULL,
	"PRECIO"	NUMERIC NOT NULL,
	"CILINDRADA"	NUMERIC NOT NULL,
	"COMBUSTIBLE"	INTEGER NOT NULL,
	"POTENCIA"	INTEGER NOT NULL,
	"TRACCION"	INTEGER NOT NULL,
	"CONSUMO MIXTO"	NUMERIC NOT NULL,
	"ETIQUETA"	INTEGER NOT NULL,
	"TIPO_CAMBIO"	INTEGER NOT NULL,
	"TIPO_CARROCERIA"	INTEGER NOT NULL,
	"CAPACIDAD_MALETERO"	NUMERIC NOT NULL,
	FOREIGN KEY("ETIQUETA") REFERENCES "ETIQUETAS"("ID"),
	FOREIGN KEY("TIPO_CAMBIO") REFERENCES "TIPO_CAMBIO"("ID"),
	FOREIGN KEY("COMBUSTIBLE") REFERENCES "COMBUSTIBLE"("ID"),
	FOREIGN KEY("TRACCION") REFERENCES "TRACCION"("ID"),
	FOREIGN KEY("TIPO_CARROCERIA") REFERENCES "TIPO_CARROCERIA"("ID"),
	PRIMARY KEY("ID" AUTOINCREMENT)
);
